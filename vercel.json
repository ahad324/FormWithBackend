{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
      "src": "/register",
      "dest": "./index.js"
    },
    {
      "src": "/login",
      "dest": "./index.js"
    },
    {
      "src": "/(.*)",
      "dest": "./index.js"
    }
    ]
  }
  
